{
  "manifest_version": 2,

  "name": "Single tab",
  "description": "Single tab vue js.",
  "version": "1.0",
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://cdn.jsdelivr.net https://unpkg.com; object-src 'self'",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "build/favicon.ico"

  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ]
}