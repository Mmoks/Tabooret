{
  "manifest_version": 2,

  "name": "Tabooret",
  "description": "Tabooret extension for tabs/tabsets management.",
  "version": "1.0",
  "content_security_policy": "script-src 'self' 'unsafe-eval' http://localhost:8098 https://cdn.jsdelivr.net https://unpkg.com; object-src 'self'",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "favicon.ico"

  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ]
}